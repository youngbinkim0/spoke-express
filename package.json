{
  "name": "commute-optimizer",
  "version": "1.0.0",
  "description": "NYC commute optimizer with Android widget - weather-aware bike vs transit recommendations",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "transiter:install": "curl -X POST 'http://localhost:8080/systems' -H 'Content-Type: application/json' -d '{\"id\": \"us-ny-subway\", \"config_url\": \"https://raw.githubusercontent.com/jamespfennell/transiter-ny/main/subway.yaml\"}'"
  },
  "dependencies": {
    "@hono/node-server": "^1.8.0",
    "dotenv": "^17.2.3",
    "hono": "^4.0.0",
    "node-cache": "^5.1.2"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
